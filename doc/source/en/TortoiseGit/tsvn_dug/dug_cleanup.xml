<sect1 id="tsvn-dug-cleanup">
	<title>Cleanup</title>
	<indexterm>
		<primary>cleanup</primary>
	</indexterm>
	<?dbhh topicname="HIDD_CLEAN"?>
	<para>
		 Remove untracked files from the working tree.
		 <menuchoice>
			<guimenu>TortoiseGit</guimenu>
			<guimenuitem>Cleanup</guimenuitem>
		</menuchoice>
		Cleans the working tree by recursively removing files that are not under version control,
		starting from the current directory.
	</para>
	<para>
		<guilabel>Clean All untracked file</guilabel>
	</para>
	<para>
		<guilabel>Clean Only Non-ignore untrack</guilabel>
	</para>
	<para>
		<guilabel>Clean Only Ignore file</guilabel>
	</para>
	<para>
		<guilabel>Remove Untracked Directory</guilabel>
	</para>
	
	
